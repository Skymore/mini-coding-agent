# OpenRouter API Key (supports multiple models)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Default model to use
LLM_MODEL=gemini-2.5-flash

# Available models list (comma separated)
AVAILABLE_MODELS=openai/gpt-4.1,openai/gpt-4.1-mini,openai/o3-mini,anthropic/claude-sonnet-4,anthropic/claude-opus-4,google/gemini-2.5-flash,google/gemini-2.5-pro,qwen/qwen3-235b-a22b:free,openai/gpt-4o-mini,openai/gpt-4o

# Model display names (format: model_id=Display Name)
MODEL_NAME_openai/gpt-4.1=GPT-4.1 (Latest)
MODEL_NAME_openai/gpt-4.1-mini=GPT-4.1 Mini
MODEL_NAME_openai/o3-mini=O3 Mini (Advanced Reasoning)
MODEL_NAME_anthropic/claude-sonnet-4=Claude Sonnet 4
MODEL_NAME_anthropic/claude-opus-4=Claude Opus 4
MODEL_NAME_google/gemini-2.5-flash=Gemini 2.5 Flash
MODEL_NAME_google/gemini-2.5-pro=Gemini 2.5 Pro
MODEL_NAME_qwen/qwen3-235b-a22b:free=Qwen3 235B A22B (Free)
MODEL_NAME_openai/gpt-4o-mini=GPT-4o Mini
MODEL_NAME_openai/gpt-4o=GPT-4o

# Model providers (format: model_id=Provider)
MODEL_PROVIDER_openai/gpt-4.1=OpenAI
MODEL_PROVIDER_openai/gpt-4.1-mini=OpenAI
MODEL_PROVIDER_openai/o3-mini=OpenAI
MODEL_PROVIDER_anthropic/claude-sonnet-4=Anthropic
MODEL_PROVIDER_anthropic/claude-opus-4=Anthropic
