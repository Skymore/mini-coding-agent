MODEL_PROVIDER_google/gemini-2.5-pro=Google
MODEL_PROVIDER_qwen/qwen3-235b-a22b:free=Chutes
MODEL_PROVIDER_openai/gpt-4o-mini=OpenAI
MODEL_PROVIDER_openai/gpt-4o=OpenAI

# Optional: Direct OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Direct Anthropic API Key
ANTHROPIC_API_KEY=your_anthropic_api_key_here